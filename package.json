    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "minimist": "^1.2.0",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "mobx-utils": "^5.5.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-measure": "^2.3.0",
    "react-outside-click-handler": "^1.3.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
