{
  "name": "Dashboard",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "repository": "github:NUbots/Dashboard",
  "engines": {
    "node": ">=10.0.0",
    "yarn": "1.x"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "yarn test --ci --runInBand",
    "test:watch": "yarn test --watch",
    "coverage": "yarn test --coverage",
    "coverage:ci": "yarn test:ci --coverage",
    "eslint": "eslint 'src/**/*.ts{,x}' --cache",
    "eslint:fix": "eslint 'src/**/*.ts{,x}' --cache --fix",
    "tscheck": "tsc --project tsconfig.test.json",
    "tscheck:watch": "yarn tsc --project tsconfig.test.json --watch",
    "dev": "nodemon ./src/server/dev.ts",
    "dev:debug": "nodemon --inspect ./src/server/dev.ts",
    "storybook": "node ./conf/storybook/dev.js",
    "storybook:build": "node ./conf/storybook/build_storybook.js",
    "storybook:prod": "node ./conf/storybook/prod.js",
    "start": "ts-node ./src/server/prod.ts",
    "build": "yarn clean:dist && webpack -p --progress --colors",
    "build:ci": "yarn clean:dist && webpack --ci --progress --colors",
    "clean": "yarn clean:dist && yarn clean:coverage",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "heroku-postbuild": "yarn build && yarn storybook:build"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-decorators": "^7.7.4",
    "@babel/plugin-syntax-jsx": "^7.7.4",
    "@babel/plugin-syntax-typescript": "^7.7.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/classnames": "^2.2.9",
    "@types/component-emitter": "^1.2.8",
    "@types/compression": "1.0.1",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/express": "^4.17.2",
    "@types/extract-text-webpack-plugin": "3.0.4",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.23",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.17",
    "@types/react": "16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-measure": "^2.0.5",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-transition-group": "^4.2.3",
    "@types/seedrandom": "^2.4.28",
    "@types/serve-favicon": "^2.5.0",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.29",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-middleware": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-loader": "^8.0.6",
    "caniuse-lite": "^1.0.30001015",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.3.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.0.0-alpha",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "nodemon": "^1.19.1",
    "postcss": "^7.0.24",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-reporter": "6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "seedrandom": "^3.0.5",
    "style-loader": "1.0.1",
    "ts-jest": "24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "url-loader": "2.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
